#include <iostream> 
#include <stack>
#include <ctype.h>
using namespace std;
long unsigned int i;

bool isOp(char a){
	if(a=='+'||a=='-'||a=='*'||a=='/'||a=='%')
		return true;
	return false;
}
int precedence(char a){
	if(a=='+'||a=='-') return 1;
	if(a=='*'||a=='/'||a=='%') return 2;
	return 0;
}
double applyOp(double num1, double num2, char op){
    switch(op){
        case '+': return num1 + num2;
        case '-': return num1 - num2;
        case '*': return num1 * num2;
        case '/': return num1 / num2;
        }
    return 0;
}
void calculate(string masukan){
	stack<double> data;
	stack<char>operasi;
	
	for(i=0; i<masukan.length(); i++){
		if(isdigit(masukan[i])){
			int value=0;
			while(i<masukan.length() && isdigit(masukan[i])){
				value = (value*10) + (masukan[i] - '0');
				i++;
			}
			i--;
			data.push(value);
		}
