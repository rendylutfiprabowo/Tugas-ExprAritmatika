long unsigned int i;

bool isOp(char a){
    if(a=='+'||a=='-'||a=='*'||a=='/'||a=='%')
        return true;
    return false;
}
int ukuran(char a){
    if(a=='+'||a=='-') return 1;
    if(a=='*'||a=='/'||a=='%') return 2;
    return 0;
}
