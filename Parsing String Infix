#include <cmath>
#include <cstdio>
#include <vector>
#include <iostream>
#include <algorithm>
using namespace std;

int main() {
    cout<<"Print : ";
    string operand;
    getline(cin,operand);    
    int N =  operand.length();
    for(int x = 0; x < N ; x++){
        if(operand[x] == '-' && (operand[x-1] == '+' || operand[x-1] == '-' ||operand[x-1] == '*' || operand[x-1] == '/'|| operand[x-1] == '%' )){
            cout << "-1 * ";
        }
        else if (operand[x] == '-' && (operand[x-1] >= '0'|| operand[x-2] >= '0')){
            cout << " " << operand[x] << " ";
        } 
