else if (operand[x] == ')'){
            cout << " " << operand[x];
        }
        else if(operand[x] == '-' && ( operand[x-1] == '*' || operand[x-1] == '+' || operand[x-1] == '-' || operand[x-1] == '/'|| operand[x-1] == '%' )){
            cout << "-1 * ";
        }
        else if (operand[x] == '-' || operand[x] == '+' || operand[x] == '*' || operand[x] == '/' || operand[x] == '%') {
            cout << " " << operand[x] << " ";
        }
    }
    return 0;
}
